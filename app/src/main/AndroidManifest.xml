<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.phedcontractorapp">
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.CAMERA"/>
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.PHEDContractor">
        <activity android:name="com.example.phedcontractor.login"
            android:windowSoftInputMode="adjustResize"
            android:fitsSystemWindows="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.example.phedcontractor.register"
            android:theme="@style/AppFullScreenTheme"/>
        <activity android:name="com.example.phedcontractor.Register_agency_details"
            android:theme="@style/AppFullScreenTheme"/>
        <activity android:name="com.example.phedcontractor.Register_address_details"
            android:theme="@style/AppFullScreenTheme"/>
        <activity android:name="com.example.phedcontractor.dashboard"
            android:theme="@style/AppFullScreenTheme"/>
        <activity android:name="com.example.phedcontractorapp.complaint_list_details"
            android:theme="@style/AppFullScreenTheme"
            />

    </application>

</manifest>